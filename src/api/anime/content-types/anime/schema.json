{
  "kind": "collectionType",
  "collectionName": "animes",
  "info": {
    "singularName": "anime",
    "pluralName": "animes",
    "displayName": "Anime",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "score": {
      "pluginOptions": {},
      "type": "decimal"
    },
    "start_date": {
      "type": "date"
    },
    "end_date": {
      "type": "date"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "planned",
        "ongoing",
        "completed",
        "hiatus",
        "cancelled"
      ],
      "required": true,
      "default": "planned"
    },
    "duration": {
      "type": "time",
      "default": "00:00"
    }
  }
