{
  "kind": "collectionType",
  "collectionName": "animes",
  "info": {
    "singularName": "anime",
    "pluralName": "animes",
    "displayName": "Anime",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "score": {
      "pluginOptions": {},
      "type": "decimal",
      "default": 2
    },
    "start_date": {
      "type": "date"
    },
    "end_date": {
      "type": "date"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "planned",
        "ongoing",
        "completed",
        "hiatus",
        "cancelled"
      ],
      "required": true,
      "default": "planned"
    },
    "duration": {
      "type": "time",
      "default": "00:00"
    },
    "anime_genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::anime-genre.anime-genre",
      "inversedBy": "anime"
    },
    "anime_studios": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::anime-studio.anime-studio",
      "inversedBy": "anime"
    },
    "anime_maps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::anime-map.anime-map",
      "mappedBy": "anime"
    }
  }
}
